<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="mainScene" prefHeight="400.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="carRentalSystem.controller.mainSceneController">
   <children>
      <AnchorPane fx:id="mainSceneRight" layoutX="153.0" prefHeight="400.0" prefWidth="800" style="-fx-background-color: gray;" />
      <Pane layoutX="-5.0" prefHeight="400.0" prefWidth="158.0" style="-fx-background-color: black;">
         <children>
            <Button fx:id="mainSceneBookCarBtn" layoutY="200.0" mnemonicParsing="false" onAction="#bookedCar" prefHeight="33.0" prefWidth="158.0" text="Booked Car" />
            <Button layoutX="4.0" layoutY="156.0" mnemonicParsing="false" onAction="#availableCars" prefHeight="33.0" prefWidth="152.0" text="Availble cars">
               <cursor>
                  <Cursor fx:constant="OPEN_HAND" />
               </cursor></Button>
            <Button fx:id="closeButton" onAction="#closeButtonAction" layoutX="4.0" layoutY="243.0" mnemonicParsing="false" prefHeight="33.0" prefWidth="152.0" text="Log out" />
            <Label fx:id="currentUserText" layoutX="22.0" layoutY="352.0" prefHeight="17.0" prefWidth="123.0" text="Current user" textFill="#bcb1b1">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
         </children>
      </Pane>
   </children>
</AnchorPane>
